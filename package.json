  "private": true,
  "scripts": {
    "build": "ls public/ | grep -v index.html | xargs -I{} rm -rf public/{} && postcss --use postcss-import --use tailwindcss -o ./public/assets/styles.css ./source/styles/app.css && NODE_ENV=production webpack --config ./webpack.config.js",
    "build:local": "npm install && npm run local",
    "build:production": "npm run build",
    "cleaninstall": "rm -rf node_modules/ && rm package-lock.json && npm cache clean --force && npm i",
    "start": "NODE_ENV=development webpack serve --config ./webpack.config.js",
    "start:dev": "webpack-dev-server --inline --hot --progress",
    "update": "npm ci",
    "update:deps": "rm -rf node_modules/ && npm cache clean --force && npm i"
  },
